{
    "name": "mnhcc/zendbase",
    "description": "Helper and base for MNHcC Zend Projects",
    "version": "0.4.1-dev",
    "type": "module",
    "license": "GPL",
    "tags": [
        "zend framework",
        "zf2",
        "helper"
    ],
    "homepage": "https://github.com/MNHcC/ZendBase",
    "authors": [
        {
            "name": "Michael Hegenbarth",
            "homepage": "http://mnhcc.mn-hegenbarth.de/",
            "role": "Developer"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.1-dev"
        }
    },
    "source": {
        "type": "git",
        "url": "https://github.com/MNHcC/ZendBase.git",
        "reference": "master"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/MNHcC/Zend3bcHelper.git"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "zendframework/zend-eventmanager": "~2.6.2 || ^3.0",
        "zendframework/zend-config": "~2.6 || ^3.0",
        "zendframework/zend-mvc": "~2.7 || ^3.0",
        "mnhcc/zend3bchelper": "^0.1.0-dev || ^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "MNHcC\\": "src/"
        },
        "classmap": [
            "src/Module/BasicModuleInterface.php",
            "src/Module/BasicModule.php",
            "src/Module.php"
        ]
    }
}
