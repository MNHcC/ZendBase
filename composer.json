{
    "name": "mnhcc/zendbase",
    "description": "Helper and base for MNHcC Zend Projects",
    "version": "0.5.3-dev",
    "type": "module",
    "license": "BSD",
    "tags": [
        "zend framework",
        "zf2",
        "helper"
    ],
    "homepage": "https://github.com/MNHcC/ZendBase",
    "authors": [
        {
            "name": "Michael Hegenbarth",
            "homepage": "http://mnhcc.mn-hegenbarth.de/",
            "role": "Developer"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.5.3-dev"
        }
    },
    "source": {
        "type": "git",
        "url": "https://github.com/MNHcC/ZendBase.git",
        "reference": "master"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/MNHcC/Zend3bcHelper.git"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "zendframework/zend-eventmanager": "~2.6.2 || ^3.0",
        "zendframework/zend-config": "~2.6 || ^3.0",
        "zendframework/zend-mvc": "~2.7 || ^3.0",
        "mnhcc/zend3bchelper": "^0.4-dev || ^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5 || ^5.0",
        "zendframework/zend-test": "^2.5",
        "zendframework/zend-modulemanager": "^2.7.1",
        "zendframework/zend-serializer": "^2.8",
        "zendframework/zend-math": "^2.6 || ^3.0",
        "zendframework/zend-log": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "MNHcC\\": "src/"
        },
        "classmap": [
            "src/Module/BasicModuleInterface.php",
            "src/Module/BasicModule.php",
            "src/Module.php"
        ]
    }
}
